// -----------------------------------------------------------------------------------
void solve()
{
    ll n, m;
    cin >> n >> m;
    cout << 1ll + (max(n, m)) << endl;
}
// -----------------------------------------------------------------------------------
int main()
{
    init;
    int t;
    cin >> t;
    while (t--)
    {
        solve();
    }
    return 0;
}
